{
"manifest_version": 2,
    "name": "Word Replacer",
    "description": "Word Replacer replaces words.",
    "version": "0.2",
    "browser_action": {
    "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}